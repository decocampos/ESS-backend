{
  "name": "gerenciador-tarefas-api",
  "version": "1.0.0",
  "description": "API RESTful de gerenciamento de tarefas com Node.js, Express e Prisma (SQLite)",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "prisma:generate": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema src/prisma/schema.prisma",
    "prepare": "npm run prisma:generate",
    "pretest": "npm run prisma:push"
  },
  "keywords": [
    "express",
    "prisma",
    "sqlite",
    "tarefas",
    "api",
    "rest"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "prisma": "^5.19.1"
  }
}
